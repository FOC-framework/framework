<VerticalLayout>
  <ValidationSettings withApply="true" withAttach="true" withPrint="true" />
  <GuiTable name="LOG_TABLE" dataPath="DATAROOT" width="100%" height="-1px" openEnabled="false" addEnabled="false" deleteEnabled="false">
    <OpenForm containerWindow="same window" />
    <TableColumn name="DATE_TIME" caption="التاريخ" width="-1px" />
    <TableColumn name="EVENT_TYPE" caption="نوع التغيير" width="-1px" />
    <TableColumn name="EVENT_UNDONE" caption="الإلغاء" width="-1px" />
    <TableColumn name="USER" caption="المستخدم" width="-1px" />
    <TableColumn name="DESCRIP" caption="شرح" width="-1px" />
    <!-- <TableColumn name="ON_BEHALF_OF" caption="نيابة عن" /> 
    <TableColumn name="TITLE" caption="العنوان" /> -->
    <TableColumn name="COMMENT" caption="ملاحظات" expandRatio="0.5"/>
    <TableColumn name="CHANGES" caption="التعديلات" expandRatio="0.5" />
    <TableColumn name="EVENT_STATUS" caption="الوضع" width="-1px" />
    <TableColumn name="STATUS_ERROR" caption="الخطأ" width="-1px" />
    <TableColumn name="PREVIOUS_STAGE" caption="المرحلة السابقة" width="-1px"/>
    <TableColumn name="TARGET_STAGE" caption="المرحلة المستهدفة" width="-1px"/>
  </GuiTable>
</VerticalLayout>